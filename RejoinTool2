import os

package = "com.roblox.client"  # đổi package theo ý bạn
pid = os.popen(f"pidof {package}").read().strip()

if pid:
    result = os.system(f"kill -9 {pid}")
    if result == 0:
        print(f"Đã kill {package} (PID {pid})")
    else:
        print(f"Không thể kill {package}, cần root")
else:
    print(f"Không tìm thấy PID cho {package}")
