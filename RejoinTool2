import os

package_name = "com.google.android.apps.nbu.files"  # đổi nếu máy bạn dùng documentsui

pid = os.popen(f"pidof {package_name}").read().strip()

if pid:
    os.system(f"kill -9 {pid}")
    print(f"Đã kill {package_name} (PID {pid})")
else:
    print(f"Không tìm thấy PID cho {package_name}")
