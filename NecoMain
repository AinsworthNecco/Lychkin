local url = "https://games.roblox.com/v1/games/8737602449/servers/Public?sortOrder=Asc&limit=1"

local function testMethod(name, func)
    print("\n[TEST] " .. name)
    if func then
        local ok, res = pcall(func, { Url = url, Method = "GET" })
        if ok and res and res.Body then
            print("  ✅ Thành công! Trả về dữ liệu:")
            print(res.Body)
        else
            print("  ❌ Lỗi hoặc không có dữ liệu.")
        end
    else
        print("  ⚠️ Không tồn tại trong executor này.")
    end
end

local function testGameHttpGet()
    print("\n[TEST] game:HttpGet")
    local ok, body = pcall(function()
        return game:HttpGet(url, true)
    end)
    if ok and body then
        print("  ✅ Thành công! Trả về dữ liệu:")
        print(body)
    else
        print("  ❌ Lỗi hoặc không có dữ liệu.")
    end
end

-- Chạy test
testMethod("http.request", http and http.request)
testMethod("syn.request", syn and syn.request)
testMethod("fluxus.request", fluxus and fluxus.request)
testMethod("request", request)
testGameHttpGet()
