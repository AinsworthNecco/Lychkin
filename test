PLACE_ID = "8737602449" 
PACKAGE_NAME = "com.roblox.clieng" 

# ƒê∆∞·ªùng d·∫´n h·ªá th·ªëng v√† file t·∫°m
REAL_DB_PATH = f"/data/data/{PACKAGE_NAME}/app_webview/Default/Cookies"
TMP_DB_PATH = "/data/local/tmp/cookies_work.db" # N∆°i ch√∫ng ta s·∫Ω s·ª≠a
# ====================================================

def run_su(command):
    """Ch·∫°y l·ªánh v·ªõi quy·ªÅn Root (su) gi·ªëng file tham kh·∫£o"""
    return subprocess.run(['su', '-c', command], capture_output=True, text=True)

def get_uid():
    """L·∫•y UID ·ª©ng d·ª•ng ƒë·ªÉ fix quy·ªÅn sau khi paste l·∫°i"""
    res = run_su(f"stat -c '%u:%g' /data/data/{PACKAGE_NAME}")
    return res.stdout.strip() if res.returncode == 0 else "10100:10100"

def process_login():
    print(f"\033[1;96m[ Rokid Manager ] - Kh·ªüi ƒë·ªông quy tr√¨nh cho {PACKAGE_NAME}...\033[0m")

    # B∆Ø·ªöC 1: T·∫Øt app ƒë·ªÉ gi·∫£i ph√≥ng file database
    run_su(f"am force-stop {PACKAGE_NAME}")
    time.sleep(1)

    # B∆Ø·ªöC 2: COPY FILE RA (ƒê√¢y l√† b∆∞·ªõc b·∫°n y√™u c·∫ßu)
    print("üìÇ ƒêang copy file database ra v√πng t·∫°m...")
    run_su(f"cp {REAL_DB_PATH} {TMP_DB_PATH}")
    run_su(f"chmod 777 {TMP_DB_PATH}") # C·∫•p quy·ªÅn t·ªëi ƒëa ƒë·ªÉ Python s·ª≠a ƒë∆∞·ª£c

    # B∆Ø·ªöC 3: S·ª¨A FILE T·∫†I V√ôNG T·∫†M (D√πng th∆∞ vi·ªán Python SQLite)
    try:
        print("üîß ƒêang n·∫°p Cookie v√†o file t·∫°m...")
        conn = sqlite3.connect(TMP_DB_PATH)
        cursor = conn.cursor()
        
        # Logic x√≥a v√† ch√®n cookie chu·∫©n c·ªßa Roblox
        cursor.execute("DELETE FROM cookies WHERE host_key = '.roblox.com' AND name = '.ROBLOSECURITY'")
        
        now = int(time.time())
        sql = """INSERT INTO cookies 
                 (creation_utc, host_key, name, value, path, expires_utc, is_secure, is_httponly, last_access_utc, has_expires, is_persistent, priority, samesite, source_scheme) 
                 VALUES (?, '.roblox.com', '.ROBLOSECURITY', ?, '/', 253402300799, 1, 1, ?, 1, 1, 1, -1, 2)"""
        cursor.execute(sql, (now, MY_COOKIE, now))
        
        conn.commit()
        conn.close()
        print("‚úÖ ƒê√£ s·ª≠a xong file t·∫°m.")
    except Exception as e:
        print(f"‚ùå L·ªói khi s·ª≠a file t·∫°m: {e}")
        return

    # B∆Ø·ªöC 4: PASTE L·∫†I V√ÄO H·ªÜ TH·ªêNG
    print("üì• ƒêang paste file ƒë√£ s·ª≠a tr·ªü l·∫°i h·ªá th·ªëng...")
    uid = get_uid()
    run_su(f"cp {TMP_DB_PATH} {REAL_DB_PATH}")
    
    # Quan tr·ªçng: Tr·∫£ l·∫°i quy·ªÅn s·ªü h·ªØu cho App (chown) v√† quy·ªÅn ƒë·ªçc ghi (chmod)
    run_su(f"chown {uid} {REAL_DB_PATH}")
    run_su(f"chmod 660 {REAL_DB_PATH}")
    
    # X√≥a file nh·∫≠t k√Ω c≈© (wal/shm) ƒë·ªÉ app b·∫Øt bu·ªôc ƒë·ªçc file m·ªõi
    run_su(f"rm -f {REAL_DB_PATH}-wal")
    run_su(f"rm -f {REAL_DB_PATH}-shm")
    run_su(f"rm {TMP_DB_PATH}")

    # B∆Ø·ªöC 5: LAUNCH ROBLOX (B√™ nguy√™n logic d√≤ng 1018 t·ª´ file improvev2.py)
    print(f"\033[1;96m[ Rokid Manager ] - ƒêang Rejoin v√†o Place ID: {PLACE_ID}...\033[0m")
    server_link = f"roblox://placeID={PLACE_ID}"
    
    # B∆∞·ªõc 1: ActivitySplash (D√≤ng 1024)
    run_su(f"am start -n {PACKAGE_NAME}/com.roblox.client.startup.ActivitySplash -d {server_link}")
    time.sleep(5)

    # B∆∞·ªõc 2: ActivityProtocolLaunch (D√≤ng 1030)
    run_su(f"am start -n {PACKAGE_NAME}/com.roblox.client.ActivityProtocolLaunch -d {server_link}")
    
    print("\033[1;92m[ Rokid Manager ] - Ho√†n t·∫•t! ƒêang v√†o game...\033[0m")

if __name__ == "__main__":
    process_login()
